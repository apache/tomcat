<module type="JAVA_MODULE" version="4">
  <component name="NewModuleRootManager" inherit-compiler-output="true">
    <exclude-output />
    <content url="file://$MODULE_DIR$">
      <sourceFolder url="file://$MODULE_DIR$/java" isTestSource="false" />
      <sourceFolder url="file://$MODULE_DIR$/test" isTestSource="true" />
      <sourceFolder url="file://$MODULE_DIR$/webapps/examples/WEB-INF/classes" isTestSource="true" />

      <!-- The directories below are excluded by default as they require the FFM API which is
          available as a preview feature in Java SDK 22. If you wish to use the FFM API in the
          IDE then set your project and module SDK levels to Java 22 or later, enable preview
          features if needed, and comment out or remove the excludeFolder lines below. !-->
      <excludeFolder url="file://$MODULE_DIR$/java/org/apache/tomcat/util/net/openssl/panama" />
      <excludeFolder url="file://$MODULE_DIR$/java/org/apache/tomcat/util/openssl" />
    </content>
    <orderEntry type="inheritedJdk" />
    <orderEntry type="sourceFolder" forTests="false" />

    <!-- Use ant.jar from path variable ANT_HOME !-->
    <orderEntry type="module-library">
      <library>
        <CLASSES>
          <root url="jar://@ANT_JAR@!/" />
        </CLASSES>
        <JAVADOC />
        <SOURCES />
      </library>
    </orderEntry>

    <!-- Use built libraries from path variable TOMCAT_BUILD_LIBS !-->
    <orderEntry type="module-library">
      <library>
        <CLASSES>
            <root url="jar://@JUNIT_JAR@!/" />
        </CLASSES>
        <JAVADOC />
        <SOURCES />
      </library>
    </orderEntry>
    <orderEntry type="module-library">
      <library>
        <CLASSES>
          <root url="jar://@JDT_JAR@!/" />
        </CLASSES>
        <JAVADOC />
        <SOURCES />
      </library>
    </orderEntry>
    <orderEntry type="module-library">
      <library>
        <CLASSES>
          <root url="jar://@EASYMOCK_JAR@!/" />
        </CLASSES>
        <JAVADOC />
        <SOURCES />
      </library>
    </orderEntry>
    <orderEntry type="module-library">
      <library>
        <CLASSES>
          <root url="jar://@HAMCREST_JAR@!/" />
        </CLASSES>
        <JAVADOC />
        <SOURCES />
      </library>
    </orderEntry>
    <orderEntry type="module-library">
      <library>
        <CLASSES>
          <root url="jar://@BYTEBUDDY_JAR@!/" />
        </CLASSES>
        <JAVADOC />
        <SOURCES />
      </library>
    </orderEntry>
    <orderEntry type="module-library">
      <library>
        <CLASSES>
          <root url="jar://@OBJENESIS_JAR@!/" />
        </CLASSES>
        <JAVADOC />
        <SOURCES />
      </library>
    </orderEntry>
    <orderEntry type="module-library">
      <library>
        <CLASSES>
          <root url="jar://@BND_JAR@!/" />
        </CLASSES>
        <JAVADOC />
        <SOURCES />
      </library>
    </orderEntry>
    <orderEntry type="module-library">
      <library>
        <CLASSES>
          <root url="jar://@MIGRATION_JAR@!/" />
        </CLASSES>
        <JAVADOC />
        <SOURCES />
      </library>
    </orderEntry>
    <orderEntry type="module-library">
      <library>
        <CLASSES>
          <root url="jar://@UNBOUNDID_JAR@!/" />
        </CLASSES>
        <JAVADOC />
